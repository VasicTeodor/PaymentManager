{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "Consul": {
    "Host": "http://localhost:8500"
  },
  "HealthChecksUI": {
    "HealthChecks": [
      {
        "Name": "CARDPAYMENT SERVICE",
        "Uri": "http://localhost:5001/paymentcard/checks/health"
      }
    ],
    "EvaluationTimeOnSeconds": 10,
    "MinimumSecondsBetweenFailureNotifications": 60
  }
}
